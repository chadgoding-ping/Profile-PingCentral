# BaseURL of the PingFed service
PF_BASE_URL=https://localhost:9031

# URL of the Delegated Admin app -- Used to create the CORS Allowed-Origins setting
DELEGATOR_PUBLIC_URL=https://localhost:1443/delegator
PD_DELEGATOR_PUBLIC_HOSTNAME=localhost

# Hostname \ Port of the PingFed service being used for Delegated Admin -- Delegator uses this to construct the OIDC calls
PF_BASE_HOST=localhost
PF_BASE_PORT=9031

# PD Base DN
USER_BASE_DN=dc=example.com
